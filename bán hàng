        for (var i in dlObjects) {

            var data_push = dlObjects[i]['ecommerce'];
            window.web_event.track(data_push['object'], data_push['event'], data_push['data']);

            if (dlObjects[i]['other_event']) {
                var data_push = dlObjects[i]['other_event'];
                window.web_event.track(data_push['object'], data_push['event'], data_push['data']);
            }
        }
    });
    

    var wpCookies = ['cdp_customerIdentify'];

    wpCookies.map(function (cookieName) {
        var cookieValue = window.getWpCookie(cookieName);

        if (!cookieValue) {

            var info_customer = [];

            if (info_customer) {

                window.dataLayer.push({
                    'info_customer': info_customer
                });

                var dlObject = {};
                dlObject['extra'] = info_customer;

                setTimeout(function() {
                    window.web_event.track('user', 'identify', dlObject);
                }, 2000);

                var date = new Date();
                date.setTime(date.getTime() + (24*60*60*1000));
                var expires = "; expires=" + date.toUTCString();

                document.cookie = cookieName + "=" + (1 || "")  + expires + "; path=/";
            }
        }
    });
