    <script>
        require([
            'jquery',
            'mage/mage'
        ], function ($) {

            var dataForm = $('.form-create-account');
            var ignore = null;

            dataForm.mage('validation', {

                                ignore: ignore ? ':hidden:not(' + ignore + ')' : ':hidden'
                            }).find('input:text').attr('autocomplete', 'off');

        });
    </script>
    
    <script type="text/x-magento-init">
    {
        ".field.password": {
            "passwordStrengthIndicator": {
                "formSelector": "form.form-create-account"
            }
        }
    }

    </script>

    <script type="text/x-magento-init">
{
    ".field.required": {
        "registerValidation": {}
    },
    "*": {
        "register": {
            "generateOTP": "https://www.kidsplaza.vn/kidsplaza/account/generateotp/",
            "verifyOTP": "https://www.kidsplaza.vn/kidsplaza/account/verify/",
            "validationInput": "https://www.kidsplaza.vn/kidsplaza/account/validation/",
            "OTPTimeout": "300",
            "registerUrl": "https://www.kidsplaza.vn/rest/hcm/V1/customer/account/register/on-web",
            "confirmOtpUrl": "https://www.kidsplaza.vn/rest/hcm/V1/customer/account/register/confirm/on-site"
        }
    }
}

    </script>
    <button type="button" id="register-block" style="display: none" class="action showpopup login"
            data-trigger="trigger"></button>
    <script>
        require(
            [
                'jquery',
                'Magento_Ui/js/modal/modal'
            ],
            function ($, modal) {
                let options = {
                    responsive: true,
                    modalClass: 'modal-login-register'
                };

                let modalContent = $('.popup-login-register'),
                    btnShowPopup = $(".action.showpopup");

                modal(options, modalContent);

                btnShowPopup.on('click', function () {
                    modalContent.modal("openModal");
                });
            }
        );
    </script>
    </div>
</div>
